{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "google",
        "eslint:recommended",
        "prettier"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "requireConfigfile": false
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "max-len": 0,
        "require-jsdoc": 0
        // "indent": ["error",4]
    },
    "parser": "@babel/eslint-parser"
}